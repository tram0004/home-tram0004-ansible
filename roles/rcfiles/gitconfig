[alias]
	info = remote -v show origin
	lb = branch -a --list $1
	lg = log --abbrev-commit --pretty=oneline --topo-order
	lgg = log --graph
	rpo = remote prune origin
	st = status

[core]
	warnambiguousrefs = false
	sparsecheckout = true

[instaweb]
	httpd=webrick

[diff]
	tool = opendiff

[push]
	default = tracking

[branch]
	autosetuprebase = always
